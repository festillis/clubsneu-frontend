{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "types": ["solid-start/env", "vite/client"],
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"]
    },
    "noEmit": true,
    "isolatedModules": true,
    "lib": ["WebWorker", "DOM"]
  }
}
